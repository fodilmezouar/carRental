<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="465.0" prefWidth="415.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="locationcar.initApp.InitAppController">
   <children>
      <VBox alignment="CENTER" layoutY="24.0" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label prefHeight="63.0" prefWidth="340.0" text="Initialisez vos informations générales" wrapText="true">
               <VBox.margin>
                  <Insets bottom="50.0" left="20.0" right="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri Bold" size="18.0" />
               </font>
            </Label>
            <HBox alignment="CENTER">
               <children>
                  <VBox alignment="CENTER_LEFT" spacing="15.0">
                     <children>
                        <Label prefHeight="28.0" text="Logo Agence :">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="28.0" text="Nom de l'Agence :">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="28.0" text="Adresse:">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="28.0" text="N°TEL :">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="28.0" text="N°registre :">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" spacing="15.0">
                     <children>
                        <HBox>
                           <children>
                              <TextField fx:id="textImage">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </TextField>
                              <Button fx:id="image_file" mnemonicParsing="false" onAction="#browseImage" prefHeight="27.0" prefWidth="25.0" />
                           </children>
                        </HBox>
                        <TextField fx:id="nom_Agence">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="Adress_Agence">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="telAgence">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="emailAgence">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox prefHeight="68.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#operationAnuller" prefHeight="68.0" prefWidth="206.0" text="Annuler">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#operationOk" prefHeight="68.0" prefWidth="210.0" text="OK">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
