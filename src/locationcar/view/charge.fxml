<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="520.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="locationcar.vehicules.ChargeController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Type charge (*) :" />
                  <ComboBox fx:id="type_charge" prefHeight="35.0" prefWidth="227.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="85.0" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label prefHeight="16.0" prefWidth="181.0" text="Date dernière charge (*) :" />
                  <DatePicker fx:id="lastDate_charge" prefHeight="35.0" prefWidth="158.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Date prochaine charge (*) :" />
                  <DatePicker fx:id="dateProch_charge" prefHeight="35.0" prefWidth="158.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="202.0" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label prefHeight="16.0" prefWidth="223.0" text="kilométrage dernière charge (*) : " />
                  <TextField fx:id="lastkilo_charge" disable="true" prefHeight="35.0" prefWidth="118.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label prefHeight="16.0" prefWidth="223.0" text="kilométrage prochain charge (*) : " />
                  <TextField fx:id="kilo_charge" disable="true" prefHeight="35.0" prefWidth="118.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="210.0" prefHeight="126.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <Label text="Déscription :" />
                  <TextArea fx:id="description" prefHeight="88.0" prefWidth="256.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="310.0" prefHeight="100.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <HBox prefHeight="63.0" prefWidth="99.0" />
                  <Label prefHeight="16.0" prefWidth="125.0" text="Dépense (*) (DA) :" />
                  <TextField fx:id="depense" prefHeight="35.0" prefWidth="216.0" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" layoutX="10.0" layoutY="330.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#ajouterCharge" prefHeight="35.0" prefWidth="146.0" text="Confirmer" />
               </children>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
